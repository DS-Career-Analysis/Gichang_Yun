        "import sys\n",
        "import bareunpy as brn\n",
        "import google.protobuf.text_format as tf\n",
        "\n",
        "API_KEY = \"API_KEY",\n
        "t = brn.Tagger(API_KEY, \"localhost\", 5656)\n",
        "ds_preference_na = []\n",
        "\n",
        "for index, row in ds_df.iterrows():\n",
