        "        na = res.nouns()\n",
        "        ds_preference_na.append(na)\n",
        "    except Exception as e:\n",
        "        pass\n",
        "\n",
        "ds_preference_na = sum(ds_preference_na, [])\n",
        "set_preference_ds = set(ds_preference_na)"
